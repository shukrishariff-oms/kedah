[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/www/Kedah/backend
ExecStart=/var/www/Kedah/backend/venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/var/www/Kedah/backend/core.sock core.wsgi:application

[Install]
WantedBy=multi-user.target
